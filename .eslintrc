{
  "extends": "reactjs",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "settings": {
    "ecmascript": 6
    "jsx": true
  }
  "rules": {
    "indent": [2,2],
    "jsx-quotes": [2, "prefer-single"],
    "no-unused-vars": 0
  },
  "plugins": [
    "react"
  ]
}
